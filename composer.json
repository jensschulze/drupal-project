{
    "name": "jensschulze/drupal-project",
    "description": "Project template for Drupal 8 projects with composer. Based on drupal-composer/drupal-project.",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jensschulze/basicdrupal.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/jensschulze/config_diff_export.git",
            "no-api": true
        },
        {
            "type": "package",
            "package": {
                "name": "d3/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/d3/d3",
                    "type": "git",
                    "reference": "v3.5.17"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/highlightjs",
                "version": "9.7.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/components/highlightjs",
                    "type": "git",
                    "reference": "9.7.0"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1",
        "composer/installers": "^1.5",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/abjs": "^1.1",
        "drupal/abtestui": "^1.0@beta",
        "drupal/admin_toolbar": "^1.24",
        "drupal/autologout": "^1.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/config_split": "^1.3",
        "drupal/console": "^1.8",
        "drupal/core": "^8.5.4",
        "drupal/csv_serialization": "^1.3",
        "drupal/diff": "^1.0@RC",
        "drupal/domain": "^1.0@alpha",
        "drupal/drd": "^3.14",
        "drupal/drd_agent": "^3.7",
        "drupal/druqs": "^1.1",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/elasticsearch_connector": "^5.0@alpha",
        "drupal/entity_browser": "^1.5",
        "drupal/entity_print": "^2.0",
        "drupal/extlink": "^1.1",
        "drupal/facets": "^1.0@beta",
        "drupal/features_ui": "^3.7",
        "drupal/field_group": "^1.0@RC",
        "drupal/focal_point": "^1.0@beta",
        "drupal/google_analytics": "^2.2",
        "drupal/honeypot": "^1.27",
        "drupal/imagemagick": "^2.3",
        "drupal/imce": "^1.6",
        "drupal/jsonapi": "^1.20",
        "drupal/key": "^1.7",
        "drupal/media_entity": "^1.7",
        "drupal/media_entity_image": "^1.3",
        "drupal/monitoring": "^1.5",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/profile": "^1.0@RC",
        "drupal/queue_ui": "2.x-dev",
        "drupal/real_aes": "^2.1",
        "drupal/rest_export_nested": "^1.0@alpha",
        "drupal/restui": "^1.15",
        "drupal/search_api": "^1.8",
        "drupal/search_api_autocomplete": "^1.0",
        "drupal/security_review": "1.x-dev",
        "drupal/simple_sitemap": "^2.12",
        "drupal/styled_google_map": "^1.2",
        "drupal/twig_tweak": "^2.0",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/viewsreference": "^2.0",
        "drupal/webform": "^5.0@RC",
        "drupal/weight": "^3.0",
        "drupal/workspace": "^1.0@beta",
        "drush/drush": "^9.0",
        "jensschulze/basicdrupal": "dev-master",
        "jensschulze/config_diff_export": "dev-master",
        "webflo/drupal-finder": "^1.0",
        "webmozart/path-util": "^2.0"
    },
    "require-dev": {
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "components/highlightjs": "9.7.*",
        "d3/d3": "3.5.*",
        "drupal/config_inspector": "^1.0@beta",
        "drupal/devel": "^1.2",
        "drupal/drupal-extension": "^3.4",
        "jcalderonzumba/gastonjs": "^1.2",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^0.9",
        "phpunit/phpunit": "^6.5",
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "hirak/prestissimo": "Composer parallel install plugin",
        "pyrech/composer-changelogs": "Display changelogs after each composer update"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
